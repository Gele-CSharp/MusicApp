@model IEnumerable<EventModel>

@{
    ViewBag.Title = "Events";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
    <div class="row">
        @foreach (var item in Model)
        {
            <partial name="_EventPartial" , model="@item" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no events yet!</h3>
}